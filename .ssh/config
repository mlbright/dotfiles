SetEnv TERM=xterm-256color

Host cloud-dev
  HostName cloud-dev
  IdentityFile ~/.ssh/dev.pem
  User ubuntu

Host github-work
    HostName github.com
    User git
    IdentityFile ~/.ssh/work
    IdentitiesOnly yes

Host github-personal
    HostName github.com
    User git
    IdentityFile ~/.ssh/personal
    IdentitiesOnly yes

